		<theme>standard</theme>

		<!-- document encoding -->
		<!-- defaults to UTF-8 -->
		<encoding>UTF-8</encoding>

		<!-- protocol for script and stylesheets -->
		<!-- defaults to https -->
		<protocol>http</protocol>
		<reportName>SeleniumTestProject
			Test Report
         <![CDATA[
               
                <!--<img src = '<local project root folder>\applogo\myStoreLogo.png'/>-->
                 
            ]]>
